{
  "version": 2,
  "env": {
    "DATABASE_URL": "@database_url",
    "POSTGRES_URL": "@database_url",
    "POSTGRES_PRISMA_URL": "@database_url",
    "POSTGRES_URL_NON_POOLING": "@database_url_non_pooling",
    "PRISMA_DATABASE_URL": "@database_url"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database_url",
      "PRISMA_GENERATE_DATAPROXY": "true"
    }
  },
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
